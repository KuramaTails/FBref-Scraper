<div class="row match serieAsecondary py-3 align-items-center justify-content-center" id="`+games[i].id+`">
    <div class="col-2 logo d-flex justify-content-center">
        <img src="/imgs/Serie_A/`+h_team.name+`.svg" class="img-fluid center-block px-3 h-100">
    </div>
    <div class="col-2 d-flex justify-content-center align-items-center h-100">
        `+h_team.name+`
    </div>
    <div class="col-2 d-flex justify-content-center align-items-center h-100">
        `+h_team.score+' - '+a_team.score+`
    </div>
    <div class="col-2 d-flex justify-content-center align-items-center h-100">
        `+a_team.name+`
    </div>
    <div class="col-2 logo d-flex justify-content-center" >
        <img src="/imgs/Serie_A/`+a_team.name+`.svg" class="img-fluid center-block px-3 h-100" >
    </div>
</div>