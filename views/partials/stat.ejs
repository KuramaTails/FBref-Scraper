<div class="row text-center" >
    <!--<div class="col stat__homeValue">
        `+Math.round(h_team.statistics[col].value*100)/100+`
    </div>-->
    <div class="col stat__categoryName">
        `+h_team.statistics[col].name+`
    </div>
    <!--<div class="col stat__awayValue">
        `+Math.round(a_team.statistics[col].value*100)/100+`
    </div>-->
</div>
<div class="row text-center" id="h_team.statistics.id" >
    <div class="col progress justify-content-end p-0" style="background-color: rgb(100 100 100 / 50%);border-radius: 0px;">
        <div class="progress-bar bg-danger" role="progressbar" style="width: `+parseInt(h_team.statistics[col].value)*100/(parseInt(h_team.statistics[col].value)+parseInt(a_team.statistics[col].value))+`%;border-radius: 15px 0px 0px 15px;" aria-valuenow="15" aria-valuemin="0" aria-valuemax="100">`+(h_team.statistics[col].value!= 0 ? Math.round(h_team.statistics[col].value*100)/100 : '')+`</div>
    </div>
    <div class="col progress justify-content-start p-0" style="background-color: rgb(100 100 100 / 50%);border-radius: 0px;">
        <div class="progress-bar bg-white text-black" role="progressbar" style="width: `+ parseInt(a_team.statistics[col].value*100)/(parseInt(a_team.statistics[col].value)+parseInt(h_team.statistics[col].value))+`%;border-radius: 0px 15px 15px 0px;" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100">`+(a_team.statistics[col].value!= 0 ? Math.round(a_team.statistics[col].value*100)/100 : '')+`</div>
    </div>
</div>